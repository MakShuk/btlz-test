POSTGRES_PORT=5432
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

APP_PORT=5000

# Wildberries API Configuration
# API токен из личного кабинета Wildberries (обязательный)
WB_API_TOKEN=your_wildberries_api_token_here
# Базовый URL API Wildberries
WB_API_BASE_URL=https://common-api.wildberries.ru

# Настройки логирования
# Уровень логирования: error, warn, info, debug
LOG_LEVEL=debug
# Директория для файлов логов
LOGS_DIR=./logs
# Включить/выключить логирование в файлы (true/false)
LOGS_TO_FILE=true