POSTGRES_PORT=5432
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

APP_PORT=5000

# Wildberries API Configuration
# API токен из личного кабинета Wildberries (обязательный)
WB_API_TOKEN=your_wildberries_api_token_here
# Базовый URL API Wildberries
WB_API_BASE_URL=https://common-api.wildberries.ru

# Google Sheets Configuration
# JSON с учетными данными сервисного аккаунта Google (обязательный)
# Получите его в Google Cloud Console -> IAM & Admin -> Service Accounts
GOOGLE_CREDENTIALS_JSON={"type":"service_account","project_id":"your-project-id","private_key_id":"...","private_key":"...","client_email":"...","client_id":"...","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"..."}
# Список ID таблиц Google Sheets через запятую (обязательный)
GOOGLE_SHEET_IDS=1BxiMVs0XRA5nFMdKvBdBZjgmUUqptlbs74OgvE2upms,1BxiMVs0XRA5nFMdKvBdBZjgmUUqptlbs74OgvE2upmt
# Имя листа по умолчанию для записи данных (обязательный)
DEFAULT_SHEET_NAME=stocks_coefs
# Scopes для Google API через запятую (необязательный, по умолчанию используется только доступ к таблицам)
GOOGLE_APP_SCOPES=https://www.googleapis.com/auth/spreadsheets

# Настройки логирования
# Уровень логирования: error, warn, info, debug
LOG_LEVEL=debug
# Директория для файлов логов
LOGS_DIR=./logs
# Включить/выключить логирование в файлы (true/false)
LOGS_TO_FILE=true